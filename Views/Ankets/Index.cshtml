@model BlankViewModel

@if(Model is null)
{
    <p class="border-gray" href="/">No users matching your criteria were found. Wait a bit for them to show up.</p>
}
else
{
    @Html.Partial("Anket", @Model.Blank)
    <div class="card-body">
        <div class="d-flex justify-content-between align-items-center">
            <p><a class="btn btn-warning" href="/Ankets/Skip/@Model.User?.Id/true">Like</a></p>
            <p><a class="btn btn-warning" href="/Ankets/Skip/@Model.User?.Id">Skip</a></p>
        </div>
    </div>
}