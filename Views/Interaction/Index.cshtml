@model BlankViewModel

@if(Model is null)
{
    <p class="border-gray" href="/">Wait a bit until someone sees your profile.</p>
}
else
{
    @Html.Partial("Anket", @Model.Blank)
    <div class="card-body">
        <div class="d-flex justify-content-between align-items-center">
            <p><a class="btn btn-warning" href="/Interaction/Like/@Model?.User?.Id">Like</a></p>
            <p><a class="btn btn-warning" href="/Interaction/Dislike/@Model?.User?.Id">Dislike</a></p>
        </div>
    </div>
}